<div class="container">
    <div class="row">
        <div class="u-max-full-width">
            <table>
                <tr>
                    <th>Number</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>DOB</th>
                </tr>
                {{#each studentModel as |studentRow|}}
                    <tr>
                        <td>{{studentRow.number}}</td>
                        <td>{{studentRow.firstName}}</td>
                        <td>{{studentRow.lastName}}</td>
                        <td>{{studentRow.DOB}}</td>
                    </tr>
                {{/each}}
            </table>
        </div>
    </div>
</div>